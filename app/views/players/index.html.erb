<p id="notice"><%= notice %></p>

<h1 class='fl-w100 pubg-font'>Players</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">Name</th>
      <th colspan='3'>K:D</th>
      <th colspan='3'>Win Ration</th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= link_to player.name, player %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
